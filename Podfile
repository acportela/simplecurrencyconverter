use_frameworks!

def all_pods
    pod 'SwiftLint', '~>0.29.4'
    pod 'Alamofire', '~> 4.8.1'
    pod 'SnapKit', '~> 4.2.0'
end

target 'CurrencyConverter' do
    all_pods
end


target 'CurrencyConverterTests' do
    all_pods
    pod 'Nimble-Snapshots', '~>6.9.0'
    pod 'Quick', '~>1.3.2'
    pod 'Nimble', '~>7.3.1'
end
